{
	"name": "resizable-rc-drawer",
	"private": false,
	"version": "1.0.0",
	"type": "module",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist"
	],
	"sideEffects": [
		"**/*.css"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc --p ./tsconfig-build.json && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"prepublishOnly": "npm run build",
		"watch": "vite & tcm --watch .",
		"check:up-to-date-types": "tcm --listDifferent ."
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rc-drawer": "^7.1.0"
	},
	"devDependencies": {
		"@types/node": "^20.8.10",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.0.3",
		"eslint": "^8.45.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"glob": "^10.3.10",
		"typed-css-modules": "^0.8.0",
		"typescript": "^5.0.2",
		"typescript-plugin-css-modules": "^5.0.2",
		"vite": "^4.4.5",
		"vite-plugin-dts": "^3.6.3",
		"vite-plugin-lib-inject-css": "^1.3.0"
	},
	"description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
	"directories": {
		"lib": "lib"
	},
	"keywords": [
		"test"
	],
	"author": "Sabit Alizade",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/sabitalizade/resizable-rc-drawer/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sabitalizade/resizable-rc-drawer.git"
	},
	"dependencies": {
		"rc-drawer": "^7.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
